using System;
class Uri1114 {
  static void Main() {
    int senha = 2002;
    Console.Write("Digite a senha: ");
    int entrada = int.Parse(Console.ReadLine());
    while(entrada!=senha){
        Console.WriteLine("Senha Inv√°lida");
        Console.Write("Digite a senha: ");
        entrada = int.Parse(Console.ReadLine());
    }
    Console.WriteLine("Acesso Permitido");
  }
}
